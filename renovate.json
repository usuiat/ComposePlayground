{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranches": ["main", "unstable"],
  "packageRules": [
    {
      "matchBaseBranches": ["unstable"],
      "matchPackageNames":["androidx.compose.*"],
      "ignoreUnstable": false,
      "respectLatest": false
    }
  ],
  "automerge": true
}
